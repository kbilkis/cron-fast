{
  "name": "cron-fast",
  "version": "0.1.0",
  "description": "Lightweight, fast cron parser with timezone support - zero dependencies",
  "keywords": [
    "cron",
    "fast",
    "lightweight",
    "parser",
    "performance",
    "schedule",
    "timezone",
    "zero-dependencies"
  ],
  "license": "MIT",
  "author": "Kasparas Bilkis kasparas@bilkis.lt",
  "repository": {
    "type": "git",
    "url": "https://github.com/kbilkis/cron-fast.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@vitest/ui": "^4.0.18",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.15.1"
}
